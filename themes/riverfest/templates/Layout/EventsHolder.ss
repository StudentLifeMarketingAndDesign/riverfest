<div id="event-content" class="typography">

<h1>$MenuTitle</h1>
$Content

<% control Children %>


<div class="event-box">

<% if EventImage %>

<div class="event-image" style="background: url(<% control EventImage.SetWidth(240) %>$Filename<% end_control %>) no-repeat 0px 4px;">
<div class="event-image-mask"></div></div>
<% end_if %> 

<h2><a href="$Link">$Title</a></h2>
<p class="event">
<% if EventDate %><strong>Performing:</strong> $EventDate.Format(F d&#44; Y), $EventTime<br /><% end_if %> 
<% if Location %><strong>Location:</strong> $Location<br /><% end_if %> 
<% if EventAges %><strong>Recommended For:</strong> $EventAges<br /><% end_if %> 
<% if EventWebsite %><strong>Website:</strong>  <a href="$EventWebsite" target="_blank">$EventWebsite</a><% end_if %> </p>
<p>$Content</p> 
</div>
<% end_control %>


</div>

<div class="clear"></div>
<div id="content-bottom"></div>
</div><!--/end content wrapper div -->
    
   